/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand (a=a, b=b, out=intres);
    Nand (a=a, b=intres, out=first);
    Nand (a=intres, b=b, out=second);
    Nand (a=first, b=second, out=out);
}
